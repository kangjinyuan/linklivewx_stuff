}

/* 进度 */

.speed-box {
  width: 100%;
}

.speed-list {
  width: 580rpx;
  margin: 0rpx auto;
  padding: 0rpx 0rpx 46rpx 30rpx;
  border-left: 2rpx solid #ccc;
  position: relative;
}

.speed-list:last-child {
  border-left: 0rpx solid #ccc;
  padding-bottom: 0rpx;
}

.speed-title, .speed-dir, .speed-time {
  width: 100%;
  margin-bottom: 18rpx;
}

.speed-dir {
  font-size: 26rpx;
  line-height: 26rpx;
  color: #666;
}

.speed-time {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #999;
}

.speed-title {
  font-size: 28rpx;
  line-height: 28rpx;
  color: #333;
}

.speed-list:first-child .speed-title {
  color: #fda414;
}

.icon-speed2 {
  background-color: #fff;
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  top: 0rpx;
  left: -12rpx;
}

.icon-speed1 {
  width: 12rpx;
  height: 12rpx;
  position: absolute;
  top: 0rpx;
  left: -6rpx;
}

/* 员工列表 */

.check-task-stuff-list-box {
  width: 100%;
  overflow: hidden;
}

.check-task-stuff-list {
  width: 50%;
  float: left;
  overflow: hidden;
  position: relative;
}

.check-task-stuff-list-name {
  width: auto;
  padding-left: 22rpx;
  font-size: 28rpx;
  color: #333;
}

.check-task-stuff-list-name-sign {
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  background-color: #fda414;
}

.check-task-stuff-list-value {
  width: 100%;
  overflow: hidden;
  float: right;
}

.stuff-head-img {
  width: 80rpx;
  height: 80rpx;
  margin: 0rpx auto 20rpx;
  border-radius: 50%;
  overflow: hidden;
  font-size: 30rpx;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
}

.stuff-head-img image {
  width: 100%;
  height: 100%;
}

.stuff-name {
  width: 260rpx;
  margin: 0rpx auto;
  font-size: 26rpx;
  line-height: 26rpx;
  color: #333;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/* 折叠 */

.fold-box {
  width: 100%;
}

.fold-list-box {
  width: 630rpx;
  border-radius: 10rpx;
  overflow: hidden;
  background-color: #fff;
  padding: 0rpx 30rpx;
  margin-bottom: 30rpx;
}

.fold-list-box:last-child {
  margin-bottom: 0rpx;
}

.fold-list-title-box {
  width: 100%;
  height: 100rpx;
  color: #333;
  position: relative;
}

.flod-list-title-text, .fold-list-state {
  height: 100%;
  font-size: 28rpx;
  line-height: 100rpx;
}

.flod-list-title-text {
  width: 200rpx;
  float: left;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.fold-list-state {
  float: right;
  margin-right: 44rpx;
}

.fold-list-title-code {
  width: auto;
  max-width: 200rpx;
  height: 100rpx;
  font-size: 26rpx;
  line-height: 100rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.fold-list-item-box {
  width: 100%;
  border-top: 2rpx solid #f5f5f5;
  overflow: hidden;
}

.fold-list {
  width: 100%;
  height: 80rpx;
}

.fold-list-text {
  width: 450rpx;
  float: left;
  height: 100%;
  font-size: 28rpx;
  line-height: 80rpx;
  color: #333;
}

.fold-list .fold-list-state {
  line-height: 80rpx;
}

/* 投影 */

.box-shadow {
  box-shadow: 0rpx 0rpx 25rpx 6rpx #f9eedc;
}

/* 操作输入框 */

.opera-box {
  width: 650rpx;
  margin: 0rpx auto 30rpx;
  padding: 0rpx 20rpx;
  overflow: hidden;
  border-radius: 10rpx;
  background-color: #fff;
  font-size: 28rpx;
