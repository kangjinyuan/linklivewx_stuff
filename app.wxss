  margin-bottom: 16rpx;
}

.form-textarea {
  width: 100%;
  height: 176rpx;
}

.form-textarea textarea {
  width: 100%;
  height: 100%;
  font-size: 28rpx;
  line-height: 36rpx;
  color: #333;
}

.word-number {
  width: 100%;
  height: 28rpx;
  margin: 40rpx 0rpx;
  font-size: 28rpx;
  color: #999;
  text-align: right;
}

.upload-img-box {
  width: 100%;
  height: 140rpx;
}

.upload-img {
  width: 140rpx;
  height: 140rpx;
  float: left;
  margin-right: 30rpx;
