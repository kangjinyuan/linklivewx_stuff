  top: -18rpx;
  right: -18rpx;
}

.del-img image {
  width: 100%;
  height: 100%;
}

.form-list-circle {
  width: 14rpx;
  height: 14rpx;
  border-radius: 50%;
}

.form-list-circle-begin {
  background-color: #2cc70d;
}

.form-list-circle-end {
  background-color: #f74927;
}

