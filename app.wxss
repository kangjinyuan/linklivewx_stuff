/**app.wxss**/

page {
  background-color: #f5f5f5;
}

image {
  display: inline;
}

scroll-view {
  width: 100%;
  height: 100%;
  overflow: auto;
}

.container-default {
  width: 100%;
  overflow: hidden;
}

.container-white {
  background-color: #fff;
}

.container-flex {
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  overflow-x: hidden;
  overflow-y: auto;
  display: flex;
  flex-direction: column;
}

.container-full {
  position: absolute;
  top: 0;
  left: 0;
  overflow: hidden;
}

.icon {
  width: 48rpx;
  height: 48rpx;
}

.icon-location {
  width: 34rpx;
  height: 38rpx;
}

.icon-key {
  width: 36rpx;
  height: 43rpx;
}

.icon-sreach {
  width: 28rpx;
  height: 28rpx;
}

.icon-select {
  width: 14rpx;
  height: 10rpx;
}

.icon-select-line {
  width: 28rpx;
  height: 16rpx;
}

.icon-jump {
  width: 16rpx;
  height: 28rpx;
}

.icon-use-photo-ok {
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
}

.icon-use-photo-fail {
  width: 48rpx;
  height: 40rpx;
  margin: 0rpx auto;
}

.face-collection-btn {
  width: 136rpx;
  height: 136rpx;
}

.icon-camera-device {
  width: 60rpx;
  height: 50rpx;
}

.icon-call-telephone {
  width: 36rpx;
  height: 36rpx;
}

.icon-calendar-down, .icon-calendar-up {
  width: 32rpx;
  height: 18rpx;
}

.icon-check-task-urgent {
  width: 61rpx;
  height: 61rpx;
}

.icon-select-category {
  width: 35rpx;
  height: 25rpx;
}

.icon-stuff-tel {
  width: 20rpx;
  height: 34rpx;
}

.icon-fold-arrow {
  width: 24rpx;
  height: 14rpx;
}

.icon image, .icon-dw image, .icon-speed image, .icon-comment image,
.icon-logo image, .no-data image, .mine-info-bg image, .headimg image,
.icon-sreach image, .icon-select image, .icon-select-line image, .icon-key image,
.icon-call-telephone image, icon-calendar-down image, icon-calendar-up image,
.icon-check-task-urgent image, .icon-select-category image, .fixed-btn image,
.icon-fold-arrow image {
  width: 100%;
  height: 100%;
}

.icon-left {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  margin: auto auto;
}

.icon-center {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto auto;
}

.icon-right {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto auto;
}

.icon-bottom {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 30rpx;
  margin: auto auto;
}

.icon-top-right {
  position: absolute;
  top: 0;
  right: 0;
}

.icon-top-left {
  position: absolute;
  top: 0;
  left: 0;
}

.icon-bottom-left {
  position: absolute;
  bottom: 0;
  left: 0;
}

.icon-bottom-right {
  position: absolute;
  bottom: 0;
  right: 0;
}

/* 公共列表 */

.common-list-box, .common-info-box {
  width: 690rpx;
  padding: 30rpx;
}

.common-list {
  width: auto;
  margin-bottom: 30rpx;
  overflow: hidden;
  padding: 30rpx;
  border-radius: 10rpx;
  background-color: #fff;
  position: relative;
}

.common-work-order-list {
  padding: 40rpx 20rpx;
}

.common-list:last-child {
  margin-bottom: 0rpx;
}

.common-list-title-box {
  width: 100%;
  overflow: hidden;
  position: relative;
  margin-bottom: 30rpx;
}

.common-list-work-order-title-box {
  padding-bottom: 30rpx;
  border-bottom: 2rpx solid #f5f5f5;
}

.common-list-title {
  border-bottom: 2rpx solid #f5f5f5;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  position: relative;
}

.common-list-title-order {
  width: 100%;
  height: 48rpx;
  padding-bottom: 30rpx;
  font-size: 32rpx;
  line-height: 48rpx;
  color: #333;
  text-indent: 66rpx;
}

.common-list-title-work-order {
  max-width: 440rpx;
  border-bottom: 0rpx solid #f5f5f5;
  font-size: 34rpx;
  line-height: 34rpx;
  color: #333;
  font-weight: 500;
  float: left;
}

.common-list-type {
  padding: 6rpx 12rpx;
  float: left;
  margin-left: 20rpx;
  border-radius: 6rpx;
  border: 2rpx solid #fda414;
  font-size: 22rpx;
  line-height: 22rpx;
  text-align: center;
  color: #fda414;
}

.rob-btn {
  width: 120rpx;
  height: 56rpx;
  font-size: 28rpx;
  line-height: 56rpx;
  color: #fda414;
  text-align: center;
}

.common-list-state {
  width: auto;
  height: auto;
  font-size: 28rpx;
  color: #fda414;
}

.common-list-work-order-state {
  width: auto;
  height: auto;
  font-size: 26rpx;
  position: absolute;
  bottom: 40rpx;
  right: 22rpx;
}

.rob-btn {
  color: #fff;
  border-radius: 28rpx;
  background-color: #fda414;
}

.common-list-content-box, .common-info-content-box {
  width: 100%;
  overflow: hidden;
  position: relative;
}

.common-list-text, .common-list-time, .common-list-dir {
  width: 580rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 28rpx;
  line-height: 28rpx;
  color: #666;
  margin-bottom: 20rpx;
}

.common-list-text:last-child {
  margin-bottom: 0rpx;
}

.common-list-dir {
  font-size: 26rpx;
  color: #999;
}

.common-list-time {
  font-size: 24rpx;
  color: #999;
}

.common-info-content {
  width: 100%;
  overflow: hidden;
  font-size: 28rpx;
  line-height: 36rpx;
  color: #333;
  text-align: justify;
}

.common-info-content-box .image-box {
  width: 100%;
  margin-top: 30rpx;
  overflow: hidden;
}

.image-box image {
  display: block;
  width: 140rpx;
  height: 140rpx;
  float: left;
  margin-right: 20rpx;
  border-radius: 10rpx;
}

.common-info-box .address-box {
  width: 578rpx;
  height: 38rpx;
  margin-top: 30rpx;
  padding-left: 52rpx;
  font-size: 26rpx;
  line-height: 38rpx;
  color: #333;
  position: relative;
}

.common-info-title-box {
  width: 100%;
  margin-bottom: 40rpx;
  font-size: 32rpx;
  line-height: 32rpx;
  color: #333;
  font-weight: 500;
}

.common-info-time {
  width: 100%;
  margin-bottom: 40rpx;
  font-size: 26rpx;
  line-height: 26rpx;
  color: #333;
}

.common-info-alert {
  width: 100%;
  margin-top: 20rpx;
  font-size: 24rpx;
  line-height: 24rpx;
  color: #666;
}

/* 计划任务和工单 */

.check-task-list-border0 {
  border-left: 10rpx solid #fd772e;
}

.check-task-list-border1 {
  border-left: 10rpx solid #61b1fc;
}

.check-task-list-border2 {
  border-left: 10rpx solid #fcb214;
}

.check-task-list-border3 {
  border-left: 10rpx solid #af6cfb;
}

.check-task-list-border4 {
  border-left: 10rpx solid #f768b2;
}

.check-task-state0 {
  color: #fd772e;
}

.check-task-state1 {
  color: #61b1fc;
}

.check-task-state2 {
  color: #fcb214;
}

.check-task-state3 {
  color: #af6cfb;
}

.check-task-state4 {
