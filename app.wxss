}

.form-list-value switch {
  width: auto;
  height: 60rpx;
}

.form-content-box {
  width: 100%;
  overflow: hidden;
  padding: 34rpx 0rpx 34rpx;
}

.form-content-title {
  width: 100%;
  height: 62rpx;
  font-size: 30rpx;
  color: #333;
}

.form-content-text {
  width: 100%;
  overflow: hidden;
  font-size: 28rpx;
  line-height: 38rpx;
