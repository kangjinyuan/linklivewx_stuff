  overflow: hidden;
  background-color: #fff;
  padding: 0rpx 30rpx;
}

.category-list {
  width: 100%;
  height: 88rpx;
  border-bottom: 2rpx solid #f5f5f5;
  font-size: 28rpx;
  line-height: 88rpx;
  color: #333;
  position: relative;
}

.category-list:last-child {
  border-bottom: none;
}

