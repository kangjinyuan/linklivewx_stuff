}

.opera-input:last-child {
  border-bottom: none;
}

.opera-input input {
  width: 586rpx;
  height: 100%;
  font-size: 28rpx;
  line-height: 100rpx;
  color: #333;
}

.opera-box .full-input {
  width: 100%;
}

/* 空页面 */

.no-data-box {
  width: 100%;
  height: 100%;
  background-color: #fafafa;
  position: relative;
}

.no-data {
  width: 330rpx;
  height: 380rpx;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto auto;
}

.no-data view {
  width: 100%;
}

.icon-community-no-data {
  height: 196rpx;
}

.icon-order-no-data {
  height: 210rpx;
}

.icon-key-no-data {
  height: 236rpx;
}

.icon-common-no-data {
  height: 220rpx;
}

.no-data .text {
  width: 100%;
  height: 170rpx;
  font-size: 28rpx;
  line-height: 170rpx;
  text-align: center;
  color: #999;
}

/* 日历 */

.calendar-box {
  width: 100%;
  padding: 30rpx 0rpx;
  background-color: #fda414;
}

.calendar-title-box {
  width: 690rpx;
  margin: 0rpx auto;
  height: 32rpx;
  margin-bottom: 40rpx;
}

.calendar-title-date, .calendar-title-today {
  width: auto;
  height: 100%;
  font-weight: 500;
  line-height: 32rpx;
  color: #fff;
}

.calendar-title-date {
  font-size: 32rpx;
  float: left;
}

.calendar-title-today {
  font-size: 30rpx;
  float: right;
}

.week-box, .date-box {
  width: 100%;
  overflow: hidden;
}

.week-box view, .date-key {
  width: 14.28%;
  height: 52rpx;
  font-size: 32rpx;
  text-align: center;
  color: #fff;
  float: left;
}

.date-key {
  height: 64rpx;
  margin: 0rpx 0rpx 10rpx;
}

.date-key view {
  width: 64rpx;
  height: 64rpx;
  font-size: 28rpx;
  border-radius: 50%;
  margin: 0rpx auto;
  line-height: 64rpx;
}

.date-key .selected {
  color: #fda414;
  background-color: #fff;
}

.calendar-switch-box {
  width: 100%;
  height: 18rpx;
  margin-top: 20rpx;
  position: relative;
}

/* 表单 */

.form-box {
  width: 100%;
  overflow: hidden;
}

.module-box {
  width: 100%;
  overflow: hidden;
  background-color: #fff;
  margin-bottom: 20rpx;
}

.module-box:last-child {
  margin-bottom: 0rpx;
}

.module {
  width: 690rpx;
  margin: 0rpx auto;
  overflow: hidden;
}

.form-list {
  width: 100%;
  overflow: hidden;
  border-bottom: 2rpx solid #f5f5f5;
  position: relative;
}

.form-list:last-child {
  border-bottom: none;
}

.form-list-name {
  width: auto;
  height: 104rpx;
  float: left;
  font-size: 32rpx;
  line-height: 104rpx;
  color: #333;
}

.form-margin-left {
  margin-left: 34rpx;
}

.form-margin-right {
  margin-right: 36rpx;
}

.form-list-name text {
