  top: 194rpx;
  right: 30rpx;
  z-index: 99;
  display: flex;
  flex-direction: column;
}

.letter-box view {
  width: 100%;
  overflow: hidden;
  font-size: 28rpx;
  color: #333;
  text-align: center;
  -webkit-box-flex: 1;
  -moz-box-flex: 1;
  flex: 1;
  -webkit-flex: 1;
}

.letter-list-box {
  width: 690rpx;
  padding: 0rpx 30rpx;
  overflow: hidden;
}

.letter-list {
  width: 100%;
  overflow: hidden;
  margin-bottom: 30rpx;
}

.letter-list:last-child {
  margin-bottom: 0rpx;
}

.letter-list-code {
  width: 100%;
  font-size: 36rpx;
  font-weight: 500;
  color: #333;
}

.letter-list-value-box {
  width: 100%;
  overflow: hidden;
}

.letter-list-value {
  width: 100%;
  overflow: hidden;
  position: relative;
}

.letter-list-head-img {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
  font-size: 30rpx;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  margin: auto auto;
}

.letter-list-user-box {
  width: 590rpx;
  padding: 22rpx 0rpx;
  border-bottom: 2rpx solid #f5f5f5;
  float: right;
}

.letter-list-user-name, .letter-list-user-telephone {
  width: 100%;
  height: 40rpx;
  font-size: 30rpx;
  line-height: 40rpx;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.letter-list-user-telephone {
  font-size: 26rpx;
  color: #666;
}

.icon-call-telephone {
  right: 56rpx;
}
