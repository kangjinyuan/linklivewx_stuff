.tab-box {
  width: 100%;
  height: 88rpx;
  background-color: #fff;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 20;
}

.tab-list {
  width: 50%;
  height: 86rpx;
  float: left;
  font-size: 30rpx;
  line-height: 86rpx;
  color: #333;
  text-align: center;
}

.tab-select {
  color: #fda414;
  border-bottom: 2rpx solid #fda414;
}

.order-box {
  margin-top: 88rpx;
}
